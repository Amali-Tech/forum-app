{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sov.comp.amalitech.forum",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"GeneralService": {
				"uri": "/odata/v4/service/thread/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sov.comp.amalitech.forum.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.108.9",
			"libs": {
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ui.unified": {},
				"sap.m": {}
			}
		},
		"handleValidation": true,
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"generalThread": {
				"type": "sap.ui.model.odata.v4.ODataModel",
				"dataSource": "GeneralService",
				"preload": true,
				"settings": {
					"synchronizationMode": "None",
					"groupId": "$direct",
					"metadataUrlParams": {
						"sap-documentation": "heading"
					}
				}
			},
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sov.comp.amalitech.forum.i18n.i18n"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "sov.comp.amalitech.forum.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "main",
					"target": "main"
				},
				{
					"pattern": "detail",
					"detail":"detail",
					"target": "detail"
				}
			],
			"targets": {
				"main": {
					"viewId": "main",
					"viewName": "Main"
				},
				"detail":{
					"viewId": "detail",
					"viewName": "Detail"
				}
			}
		}
	}
}